{% extends "./proj/bg-base.html.twig" %}

{% block title %}Leaderboard{% endblock %}

{% block body %}
<div>
   
<table id="leaderboard">
    <caption>Leaderboard</caption>
    <thead>
        <tr>
            <th>Namn</th>
            <th>Alias</th>
            <th>Score</th>
        </tr>
    </thead>
    <tbody>
        {% for item in leaderboard %}
        <tr>
            <td>{{ item.namn }}</td>
            <td>{{ item.alias }}</td>
            <td>{{ item.score }}</td>
        </tr>
        
        {% endfor %}
    </tbody>

</table>

</div>
{% endblock %}
